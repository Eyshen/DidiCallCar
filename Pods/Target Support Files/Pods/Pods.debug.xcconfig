ARCHS = $(ARCHS_STANDARD), armv7s $(ARCHS_STANDARD)
FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/AMapNavi" "$(PODS_ROOT)/AMapSearch"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AMapNavi" "${PODS_ROOT}/Headers/Public/AMapSearch" "${PODS_ROOT}/Headers/Public/HHAlertView" "${PODS_ROOT}/Headers/Public/MBProgressHUD"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AMapNavi" -isystem "${PODS_ROOT}/Headers/Public/AMapSearch" -isystem "${PODS_ROOT}/Headers/Public/HHAlertView" -isystem "${PODS_ROOT}/Headers/Public/MBProgressHUD"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Pods-HHAlertView" -l"Pods-MBProgressHUD" -l"stdc++.6.0.9" -l"z" -framework "AMapNaviKit" -framework "AMapSearchKit" -framework "CoreGraphics" -framework "CoreLocation" -framework "CoreTelephony" -framework "QuartzCore" -framework "Security" -framework "SystemConfiguration"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods